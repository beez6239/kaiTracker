    <!-- TradingView Widget Embed with Fullscreen Option -->
    <div class="w-full flex flex-col items-center py-6 relative">
         
        <div id="tv-chart-container" class="w-full" style="max-width: 900px;">
            <iframe id="tv-iframe"
                src="https://www.tradingview.com/widgetembed/?symbol=BINANCE:ETHUSDT&interval=1D&theme=dark"
                width="100%" height="400" frameborder="0" allowtransparency="true" scrolling="no"
                style="border-radius: 8px;"></iframe>
            <button id="tv-fullscreen-btn"
            class="absolute right-4 top-4 z-10 bg-gray-800 text-white px-3 py-1 rounded hover:bg-gray-700 text-xs"
            onclick="toggleTradingViewFullscreen(event)">
            Full Screen
        </button>
        </div>
    </div>
</div>
<script>
function toggleTradingViewFullscreen(e) {
    e.preventDefault();
    const container = document.getElementById('tv-chart-container');
    const iframe = document.getElementById('tv-iframe');
    const btn = document.getElementById('tv-fullscreen-btn');
    if (!document.fullscreenElement) {
        container.requestFullscreen().then(() => {
            iframe.style.height = "100vh";
            btn.textContent = "Exit Full Screen";
        });
    } else {
        document.exitFullscreen().then(() => {
            iframe.style.height = "400px";
            btn.textContent = "Full Screen";
        });
    }
}
// Handle exit from fullscreen via ESC or browser controls
document.addEventListener('fullscreenchange', function () {
    const iframe = document.getElementById('tv-iframe');
    const btn = document.getElementById('tv-fullscreen-btn');
    if (!document.fullscreenElement) {
        iframe.style.height = "400px";
        btn.textContent = "Full Screen";
    }
});
</script>
